create or replace type chesnokov_se.t_speciality is object
(
    id_speciality    number,
    speciality_name  varchar2(100),
    required_min_age number,
    required_max_age number,
    delete_dt        date,
    constructor function t_speciality
    (
        speciality_name  varchar2,
        required_min_age number,
        required_max_age number
    ) return self as result,
    member function get_id_speciality return number,
    member function get_speciality_name return varchar2,
    member function get_required_min_age return number,
    member function get_required_max_age return number
);

create sequence t_speciality_s;

create or replace type body chesnokov_se.t_speciality
as
    constructor function t_speciality(
        speciality_name  varchar2,
        required_min_age number,
        required_max_age number
    ) return self as result
    as
        next_id number := t_speciality_s.nextval;
    begin
        self.id_speciality := next_id;
        self.speciality_name := speciality_name;
        self.required_min_age := required_min_age;
        self.required_max_age := required_max_age;
        self.delete_dt := null;
        return;
    end;

    member function get_id_speciality return number is
    begin
        return self.id_speciality;
    end;

    member function get_speciality_name return varchar2 is
    begin
        return self.speciality_name;
    end;

    member function get_required_min_age return number is
    begin
        return self.required_min_age;
    end;

    member function get_required_max_age return number is
    begin
        return self.required_max_age;
    end;
end;


create or replace type chesnokov_se.t_patient as object(
    id_patient number,
    surname_name varchar2(100),
    name varchar2(100),
    middle_name varchar2(100),
    date_birth date,
    id_gender number,
    phone_number number,
    id_area number,
    id_user number,
    constructor function t_patient
    (
        surname_name  varchar2,
        name varchar2,
        middle_name varchar2 default null,
        date_birth date,
        id_gender number,
        phone_number number default null,
        id_area number,
        id_user number
    ) return self as result,
    member function get_surname_name return varchar2,
    member function get_name return varchar2,
    member function get_middle_name return varchar2,
    member function get_date_birth return date,
    member function get_id_gender return number,
    member function get_phone_number return number,
    member function get_id_area return number,
    member function get_id_user return number
);

create sequence t_patient_s;

create or replace type body chesnokov_se.t_patient
as
    constructor function t_patient(
        surname_name  varchar2,
        name varchar2,
        middle_name varchar2 default null,
        date_birth date,
        id_gender number,
        phone_number number default null,
        id_area number,
        id_user number
    ) return self as result
    as
        next_id number := t_patient_s.nextval;
    begin
        self.ID_PATIENT := next_id;
        self.surname_name := surname_name;
        self.name := name;
        self.middle_name := middle_name;
        self.date_birth := date_birth;
        self.id_gender := id_gender;
        self.phone_number := phone_number;
        self.id_area := id_area;
        self.id_user := id_user;
        return;
    end;

    member function get_surname_name return varchar2 is
    begin
        return self.surname_name;
    end;

    member function get_name return varchar2 is
    begin
        return self.name;
    end;

    member function get_middle_name return varchar2 is
    begin
        return self.middle_name;
    end;

    member function get_date_birth return date is
    begin
        return self.date_birth;
    end;

    member function get_id_gender return number is
    begin
        return self.id_gender;
    end;

    member function get_phone_number return number is
    begin
        return self.phone_number;
    end;

    member function get_id_area return number is
    begin
        return self.id_area;
    end;

    member function get_id_user return number is
    begin
        return self.id_user;
    end;
end;


create or replace type chesnokov_se.t_document as object
(
    id_document number,
    name varchar2(100),
    constructor function t_document(
        name varchar2
    ) return self as result,
    member function get_id_document return number,
    member function get_name return varchar2
);

create sequence t_document_s;

create or replace type body chesnokov_se.t_document
as
    constructor function t_document(
        name  varchar2
    ) return self as result
    as
        next_id number := t_document_s.nextval;
    begin
        self.id_document := next_id;
        self.name := name;
        return;
    end;

    member function get_id_document return number is
    begin
        return self.id_document;
    end;

    member function get_name return varchar2 is
    begin
        return self.name;
    end;
end;


create or replace type chesnokov_se.t_document_number as object(
    id_document_number number,
    id_patient number,
    id_document number,
    value_doc varchar2(100),
    constructor function t_document_number
    (
        id_patient number,
        id_document number,
        value_doc varchar2
    ) return self as result,
    member function get_id_document_number return number,
    member function get_id_patient return number,
    member function get_id_document return number,
    member function get_value_doc return varchar2
);

create sequence t_document_number_s;

create or replace type body chesnokov_se.t_document_number
as
    constructor function t_document_number(
        id_patient number,
        id_document number,
        value_doc varchar2
    ) return self as result
    as
        next_id number := t_document_number_s.nextval;
    begin
        self.id_document_number := next_id;
        self.id_patient := id_patient;
        self.id_document := id_document;
        self.value_doc := value_doc;
        return;
    end;

    member function get_id_document_number return number is
    begin
        return self.id_document_number;
    end;
    member function get_id_patient return number is
    begin
        return self.id_patient;
    end;
    member function get_id_document return number is
    begin
        return self.id_document;
    end;
    member function get_value_doc return varchar2 is
    begin
        return self.value_doc;
    end;
end;


create or replace type chesnokov_se.t_patient_extendend as object(
    patient t_patient,
    document_number t_document_number,
    constructor function t_patient_extendend
    (
        patient t_patient,
        document_number t_document_number
    ) return self as result,
    constructor function t_patient_extendend
    (
        surname_name  varchar2,
        name varchar2,
        middle_name varchar2 default null,
        date_birth date,
        id_gender number,
        phone_number number default null,
        id_area number,
        id_user number,
        id_document_number number,
        id_patient number,
        id_document number,
        value_doc varchar2
    ) return self as result
);

create or replace type body chesnokov_se.t_patient_extendend
as
    constructor function t_patient_extendend(
        patient t_patient,
        document_number t_document_number
    ) return self as result
    as
    begin
        self.patient := patient;
        self.document_number := document_number;
        return;
    end;

    constructor function t_patient_extendend(
        surname_name  varchar2,
        name varchar2,
        middle_name varchar2 default null,
        date_birth date,
        id_gender number,
        phone_number number default null,
        id_area number,
        id_user number,
        id_document_number number,
        id_patient number,
        id_document number,
        value_doc varchar2
    ) return self as result
    as
    begin
        self.patient := chesnokov_se.t_patient(
            surname_name,
            name,
            middle_name,
            date_birth,
            id_gender,
            phone_number,
            id_area,
            id_user
        );
        self.document_number := t_document_number(
            id_document_number,
            id_patient,
            id_document,
            value_doc
        );
        return;
    end;
end;


create or replace type chesnokov_se.t_doctor as object(
    id_doctor number,
    id_hospital number,
    id_area number,
    id_qualification number,
    doctor_name varchar2(100),
    salary number,
    education varchar2(100),
    delete_dt date,
    constructor function t_doctor
    (
        id_hospital number,
        id_area number,
        id_qualification number,
        doctor_name varchar2,
        salary number,
        education varchar2
    ) return self as result,
    member function get_id_doctor return number,
    member function get_id_hospital return number,
    member function get_id_area return number,
    member function get_id_qualification return number,
    member function get_doctor_name return varchar2,
    member function get_salary return number,
    member function get_education return varchar2
);

create sequence chesnokov_se.t_doctor_s;

create or replace type body chesnokov_se.t_doctor
as

    constructor function t_doctor(
        id_hospital number,
        id_area number,
        id_qualification number,
        doctor_name varchar2,
        salary number,
        education varchar2
    ) return self as result
    as
        next_id number := t_doctor_s.nextval;
    begin
        self.id_doctor := next_id;
        self.id_hospital := id_hospital;
        self.id_area := id_area;
        self.id_qualification := id_qualification;
        self.doctor_name := doctor_name;
        self.salary := salary;
        self.education := education;
        return;
    end;

    member function get_id_doctor return number is
    begin
        return self.id_doctor;
    end;

    member function get_id_hospital return number is
    begin
        return self.id_hospital;
    end;

    member function get_id_area return number is
    begin
        return self.id_area;
    end;

    member function get_id_qualification return number is
    begin
        return self.id_qualification;
    end;

    member function get_doctor_name return varchar2 is
    begin
        return self.doctor_name;
    end;

    member function get_salary return number is
    begin
        return self.salary;
    end;

    member function get_education return varchar2 is
    begin
        return self.education;
    end;
end;


