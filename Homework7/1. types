create or replace type chesnokov_se.t_working_time as object(
    id_time number,
    day number,
    begin_time timestamp,
    end_time timestamp,
    id_hospital number,
    constructor function t_working_time
    (
        day number,
        begin_time timestamp,
        end_time timestamp,
        id_hospital number
    ) return self as result,
    member function get_day return number,
    member function get_begin_time return timestamp,
    member function get_end_time return timestamp ,
    member function get_id_hospital return number
);

create sequence t_working_time_s;

create or replace type body chesnokov_se.t_working_time
as
    constructor function t_working_time(
        day number,
        begin_time timestamp,
        end_time timestamp,
        id_hospital number
    ) return self as result
    as
        next_id number := t_working_time_s.nextval;
    begin
        self.id_time := next_id;
        self.day := day;
        self.begin_time := begin_time;
        self.end_time := end_time;
        self.id_hospital := id_hospital;
        return;
    end;

    member function get_day return number is
    begin
        return self.day;
    end;

    member function get_begin_time return timestamp is
    begin
        return self.begin_time;
    end;

    member function get_end_time return timestamp is
    begin
        return self.end_time;
    end;

    member function get_id_hospital return number is
    begin
        return self.id_hospital;
    end;
end;


